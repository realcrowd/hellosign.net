{
	baseUrl: 'https://api.hellosign.com/v3',
	endpoints: 
	{
		account:
		{
			get:
			{
				method: 'GET',
				url: '/account'
			},
			update:
			{
				method: 'POST',
				url: '/account'
			},
			create:
			{
				method: 'POST',
				url: '/account/create'
			}
		},
		reusableForm:
		{
			list:
			{
				method: 'GET',
				url: '/reusable_form/list'
			},
			get:
			{
				method: 'GET',
				url: '/reusable_form/{reusable_form_id}'
			},
			addUser:
			{
				method: 'POST',
				url: '/reusable_form/add_user/{reusable_form_id}'
			},
			removeUser:
			{
				method: 'POST',
				url: '/reusable_form/remove_user/{reusable_form_id}'
			}
		},
		signatureRequest:
		{
			send:
			{
				method: 'POST',
				url: '/signature_request/send'
			},
			list:
			{
				method: 'GET',
				url: '/signature_request/list'
			},
			get:
			{
				method: 'GET',
				url: '/signature_request/{signature_request_id}'
			},
			remind:
			{
				method: 'POST',
				url: '/signature_request/remind/{signature_request_id}'
			},
			getFinalCopy:
			{
				method: 'GET',
				url: '/signature_request/final_copy/{signature_request_id}'
			},
			cancel:
			{
				method: 'POST',
				url: '/signature_request/cancel/{signature_request_id}'
			},
			sendForm:
			{
				method: 'POST',
				url: '/signature_request/send_with_reusable_form'
			},
			createEmbeddedWithReusableForm:
			{
				method: 'POST',
				url: '/signature_request/create_embedded_with_reusable_form'
			},
			createEmbedded
			{
				method: 'POST',
				url: '/signature_request/create_embedded'
			}
		},
		team:
		{
			create:
			{
				method: 'POST',
				url: '/team/create'
			},
			update:
			{
				method: 'POST',
				url: '/team'
			},
			get:
			{
				method: 'GET',
				url: '/team'
			},
			destroy:
			{
				method: 'POST',
				url: '/team/destroy'
			},
			addMember:
			{
				method: 'POST',
				url: '/team/add_member'
			},
			removeMember:
			{
				method: 'POST',
				url: '/team/remove_member'
			}
		},
		unclaimedDraft:
		{
			create:
			{
				method: 'POST',
				url: '/unclaimed_draft/create'
			}
		},
		embedded:
		{
			getSignUrl:
			{
				method: 'GET',
				url: '/embedded/sign_url/{signature_id}'
			}
		}
	}
}